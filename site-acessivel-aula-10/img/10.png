  vertical-align: top;
  border-radius: 50%;
}

.checkbox-container input{
  display: none;
}

.checkbox-container input:checked ~ .checkbox {
  opacity: 1;
}

.checkbox-container input:checked ~ .checkbox .checkbox-icon:after {
  display: block;
}

